    "plop": "^2.5.3",
    "pre-commit": "^1.2.2",
    "prettier": "2.0.2",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "shelljs": "^0.8.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^2.3.2",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
