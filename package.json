{
  "name": "salad-bowl",
  "version": "0.0.1",
  "description": "Online implementation of Salad Bowl",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/GentlemenoftheCode/salad-bowl"
  },
  "keywords": [
    "spyfall"
  ],
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "https://github.com/orgs/GentlemenoftheCode/people",
  "license": "ISC",
  "homepage": "",
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=development BUILD=true webpack --config config/webpack.dev.babel.js --color --progress --hide-modules --display-optimization-bailout",
    "build-prod": "cross-env NODE_ENV=production BUILD=true webpack --config config/webpack.prod.babel.js --color --progress --hide-modules --optimize-minimize --optimize-dedupe --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "firebase-deploy": "firebase deploy --only hosting -P default",
    "firebase-deploy-prod": "rm ./build/*.js.map ; firebase deploy --only hosting -P prod",
    "release": "npm run build && npm run firebase-deploy",
    "release-prod": "npm run build-prod && npm run firebase-deploy-prod",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --config config/webpack.dev.babel.js --port 4000 --host 0.0.0.0",
    "clean": "npm run test:clean && npm run build:clean",
    "lint": "npm run prettier",
    "deploy": "cp app/translations.js functions/lib/translations.js && firebase deploy --project default --only",
    "deploy-prod": "cp app/translations.js functions/lib/translations.js && firebase deploy --project prod --only",
    "copy-translations": "rimraf ./build/i18n && ncp ./public/i18n ./build/i18n",
    "deploy-translations": "npm run copy-translations && npm run firebase-deploy",
    "deploy-translations-prod": "npm run copy-translations && npm run firebase-deploy-prod",
    "prettier": "prettier --config .prettierrc --write \"app/**/*.js\""
  },
  "pre-commit": "prettier",
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "animate.css": "^3.7.2",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.1",
    "emotion": "^10.0.27",
    "firebase": "^7.7.0",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.10.1",
    "i18next": "^17.3.1",
    "i18next-browser-languagedetector": "^3.1.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-animated-css": "^1.2.1",
    "react-dropzone": "^10.2.1",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-html-parser": "^2.0.2",
    "react-i18next": "^10.13.2",
    "react-icons": "^3.8.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-spinners": "^0.5.12",
    "react-toastify": "^5.5.0",
    "reactstrap": "^8.4.0",
    "redux": "^4.0.5",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "sanitize.css": "^10.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@ensemblebr/eslint-config": "^1.0.1",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-emotion": "^10.0.27",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-runtime": "^6.26.0",
    "circular-dependency-plugin": "^5.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.1",
